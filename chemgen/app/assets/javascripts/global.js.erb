function getCount(e,v){
  if(e!='' && v!='') $('#slidecounter'+e).text(v);
  $.ajax({
    beforeSend:function(xhr){
      xhr.setRequestHeader('X-CSRF-Token',$('meta[name="csrf-token"]').attr('content'))
    },
    data:'m='+$('#slidecounter1').text()+'&v='+$('#slidecounter2').text()+'&psa='+$('#slidecounter3').text()+'&logp='+$('#slidecounter4').text()+'&rb='+$('#slidecounter5').text()+'&lr='+$('#slidecounter6').text(),
    dataType:'script',
    type:'get',
    url:'/search'
  }).fail(function(r,s,e){
    alert('Oops. There was an error. Please refresh the page and try again.');
  });
}
function gotoresults(){
  window.location='/search?m='+$('#slidecounter1').text()+'&v='+$('#slidecounter2').text()+'&psa='+$('#slidecounter3').text()+'&logp='+$('#slidecounter4').text()+'&rb='+$('#slidecounter5').text()+'&lr='+$('#slidecounter6').text();
  return false;
}
